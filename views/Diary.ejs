<%- include('./layout/header') %>
<%- include('./layout/navbar') %>

    <div class="add-container">
        <div class="add">
            <p>Add to Diary</p>
            <a href="/add">Add</a>
        </div>
    </div>

<div class="diary">
    <% data.forEach((diary)=>{%>
        <div class="diary-container">
            <h3><%= diary.title %></h3>
            <p><%= diary.date %></p>
            <a href="/diary/<%= diary.id %>">More</a>
        </div>
    <%}) %>
</div>
<%- include('./layout/footer') %>